LOAD DATA  INDDN LOADIN
RESUME YES
LOG NO NOCOPYPEND
WORKDDN (SYSUT1,SORTOUT)
ENFORCE NO
ERRDDN SYSERR
INTO TABLE VENTAS
(
    TVENT_COD_VENTA     POSITION(1:6) DECIMAL EXTERNAL ,
    TVENT_COD_VENDEDOR  POSITION(7:10) DECIMAL EXTERNAL ,
    TVENT_COD_PRODUCTO  POSITION(11:14) DECIMAL EXTERNAL ,
    TVENT_FECHA         POSITION(15:22) CHARACTER ,
    TVENT_KILOS         POSITION(23:26) DECIMAL EXTERNAL
)
