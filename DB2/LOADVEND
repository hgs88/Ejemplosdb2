//SYSPRINT DD SYSOUT=*
//SYSUDUMP DD SYSOUT=*
//SYSERR DD SYSOUT=*
//UTPRINT DD SYSOUT=*
//SYSUT1 DD DSN=&&SYSUT1,DISP=(NEW,DELETE,DELETE),
// SPACE=(CYL,(10,10))
//SORTOUT DD DSN=&&SORTOUT,DISP=(NEW,DELETE,DELETE),
// SPACE=(CYL,(10,10))
//SYSMAP DD DSN=&&SYSMAP,DISP=(NEW,DELETE,DELETE),
// SPACE=(CYL,(10,10))
//LOADIN DD DSN=VERDURA.VENDEDOR.SEQ1,DISP=SHR
//**
